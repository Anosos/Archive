//sami
#include <iostream>
#include <string>
#include<iomanip>
#include <math.h>
#include<algorithm>
using namespace std;

int main() {
int t;
    cin >> t;

    for (int i = 0; i < t; i++) {
        int x;
        cin >> x;
        int reverse = 0;
        while (x > 0) {
            reverse = reverse * 10 + x % 10;
            x /= 10;
            //equation 1=6 so removed 6
        }
        cout << reverse << endl;
}
}
