//sami
#include <iostream>
#include <string>
#include<iomanip>
#include <math.h>
#include<algorithm>
using namespace std;

int main() {
string input;
    getline(cin, input);

    int wordCount = 1;
    for (int i = 0; i < input.size(); i++) {
        if (input[i] == ' ') {
            wordCount++;
        }
    }

    cout << wordCount << endl;
}
